{
    "manifest_version": 2,
    "name": "fmtl",
    "description": "A simple extension that does something useful.",
    "version": "1.0",
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["backgroundChrome.js"]
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "FMTL",
      "default_popup": "index.html"
    }
  }
